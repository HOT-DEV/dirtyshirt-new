@font-face {
    font-family: 'whitney medium';
    src: url('https://cdn.shopify.com/s/files/1/0777/6955/files/whitney-medium.ttf?30108')  format('truetype');
    src: url('{{ 'whitney-medium-webfont.woff' | asset_url }}') format('woff'),
      url('{{ 'whitney-medium-webfont.woff2' | asset_url }}') format('woff2'),
      url('{{ 'Whitney-Medium.eot' | asset_url }}') format('embedded-opentype'),
      url('https://cdn.shopify.com/s/files/1/0777/6955/files/whitney-medium.ttf?30108') format('truetype');
  }
  
  @font-face {
    font-family: 'whitney bold';
    src: url('https://cdn.shopify.com/s/files/1/0777/6955/files/whitney-bold.ttf?30108')  format('truetype'); 
    src: url('{{ 'whitney-bold-webfont.woff' | asset_url }}') format('woff'),
        url('{{ 'whitney-bold-webfont.woff2' | asset_url }}') format('woff2'),
        url('{{ 'Whitney-Bold.eot' | asset_url }}') format('embedded-opentype'),
        url('https://cdn.shopify.com/s/files/1/0777/6955/files/whitney-bold.ttf?30108') format('truetype');
  }
  
  @font-face {
    font-family: 'Knockout Sumo';
    src: url('https://cdn.shopify.com/s/files/1/0777/6955/files/Knockout-HTF54-Sumo.ttf?30132')  format('truetype'); 
    src: url('{{ 'knockout-htf54-sumo-webfont.woff' | asset_url }}') format('woff'),
        url('{{ 'knockout-htf54-sumo-webfont.woff2' | asset_url }}') format('woff2'),
        url('{{ 'Knockout-HTF54-Sumo.eot' | asset_url }}') format('embedded-opentype'),
        url('https://cdn.shopify.com/s/files/1/0777/6955/files/whitney-bold.ttf?30108') format('truetype');
  }